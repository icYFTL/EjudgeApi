{

  "edudje": {
    "requires_patch": {
      "secure_exec": true,
      "security-violation": true,
      "memory_limit": true
    },
    "inst_ejudge_path": "/opt/ejudge/bin/ejudge-execute",
    "kill_sig": "KILL",
    "is_patched": true,
    "group": "ejudge_slave"
  },
  "api": {
    "key": "kek"
  },
  "environment": {
    "solves_storage_path": "/home/ejudge/solves"
  },
  "builders": {
    "python3": "/usr/local/bin/pyinstaller",
    "c": "gcc",
    "cpp": "g++",
    "pascal": "fpc",
    "java": "javac"
  }
}